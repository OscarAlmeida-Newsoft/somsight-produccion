                <div id="btn_login_logged">

                    <p id="touch">

                        <span class="palabra_top" style="background-image: url(../../images/icono_login@_user.TenantSkin.TenantStyleLoginIconColor);"></span>
                        <span class="flecha_top"></span>
                    </p>

                    <!--================================= LOGIN  ======================================-->
                    <div id="login" class="logged_in">
                        <div class="formulario">

                            <h3>@_user.UserEmail</h3>

                            <div class="btn_idiomas">
                                @Html.Partial("_Languages")
                            </div>

                            <ul class="opciones">
                                <li><a href="@Url.Action("EditProfile", "Account" )" target="_self" class="icono profile">@TranslatorHelper.TranslateTextByIdentifier("my_profile_text")</a></li>
                                <li><a href="@Url.Action("Settings", "Account" )" target="_self" class="icono configuration">@TranslatorHelper.TranslateTextByIdentifier("configuration_text")</a></li>
                                @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", style = "display: none;" }))
                                {
                                    @Html.AntiForgeryToken()
                                }
                                <li><a href="javascript:document.getElementById('logoutForm').submit()" target="_self" class="icono log_out">@TranslatorHelper.TranslateTextByIdentifier("log_out_text")</a></li>
                            </ul>

                            <hr />

                            @if (userPlan.PlanName != "WITHOUT")
                            {
                                <h4>@TranslatorHelper.TranslateTextByIdentifier("you_are_in_the_text") <strong>@plan</strong></h4>
                            }

                            @if (userPlan.Id != _user.HighestPlanCode && _user.ShowUpgradeOption)
                            {
                                <a onclick="gAnalytics.throwRedirectEvent('Click Upgrade', 'Plan Upgrade','Plan Upgrade','@Url.Action("Upgrade", "Plan" )')" href="#" class="upgrade" style="background-color: @_user.TenantSkin.TenantStyleTertiaryBackgroundColor; color: @_user.TenantSkin.TenantStyleTertiaryFontColor; background-image: url('../images/icono_carrito@_user.TenantSkin.TenantStyleTertiaryIconColor');">@TranslatorHelper.TranslateTextByIdentifier("upgrade_your_plan_text")</a>

                            }
                        </div>

                    </div>
                    <!--================================= LOGIN  ======================================-->
                </div>






            </ul>



        </div>
        <div id="menu_superior" style="background-color: @_user.TenantSkin.TenantStylePrimaryBackgroundColor; ">
            <div class="centro">










                <ul class="pestanas">
                    <li><a style="color: @_user.TenantSkin.TenantStylePrimaryFontColor;" href="@Url.Action("Index","MyBoard")" target="_self" class="@{ if(ViewBag.Active == "MyBoard"){ ViewContext.Writer.Write("active"); } }">@TranslatorHelper.TranslateTextByIdentifier("home_text")</a></li>
                    @Html.Action("LoadModularityMenu", "Modularity", new { pActiveModuleName = ViewBag.Active })

