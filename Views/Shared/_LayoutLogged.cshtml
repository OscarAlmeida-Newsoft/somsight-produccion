                        <li><a href="@Url.Action(" Index","AssessmentBoard", new { @id=2})" class="@{ if(ViewBag.Active == " 2"){ ViewContext.Writer.Write("active"); } }" target="_self">Cloud</a></li>
                        <li><a href="@Url.Action(" Index","AssessmentBoard", new { @id=3})" class="@{ if(ViewBag.Active == " 3"){ ViewContext.Writer.Write("active"); } }" target="_self">Productivity</a></li>
                        <li><a href="@Url.Action(" Index","AssessmentBoard", new { @id=4})" class="@{ if(ViewBag.Active == " 4"){ ViewContext.Writer.Write("active"); } }" target="_self">Industry Insights</a></li>
                        <li><a href="@Url.Action(" Index","MyPlatform")" target="_self" class="@{ if(ViewBag.Active == " 5"){ ViewContext.Writer.Write("active"); } }">Measure My Platform</a></li>
                        <li><a href="#" target="_self">Software Assets</a></li>*@
                </ul>


            </div>
        </div>


        <!--================================================-->
        <!--================================================-->
        <!--================================================-->
        <!--================================================-->



    </div>
    <!--================================= HEADER ======================================-->
    <!--================================= CONTENT ======================================-->
    @RenderBody()

    <!--================================= CONTENT ======================================-->
