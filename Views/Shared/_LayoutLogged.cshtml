        <div id="footer">
            <div class="centro">
                <ul>
                    <li><a href="@Url.Action("TermsAndConditions","Home")" target="_blank">@TranslatorHelper.TranslateTextByIdentifier("terms_and_conditions_text")</a></li>
                    <li><a href="@Url.Action("DataProtection","Home")">@TranslatorHelper.TranslateTextByIdentifier("privacy_and_cookies_text")</a></li>
                </ul>


                <p>© 2010-@DateTime.Now.Year @TranslatorHelper.TranslateTextByIdentifier("powered_by_text")</p>
            </div>
        </div>
