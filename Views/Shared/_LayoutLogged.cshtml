<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")

    @Html.Partial("_GAnalyticsPartial")
    <script>
        gAnalytics.addGoogleAnalytics();
    </script>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/customjs")

    <script src="@Scripts.Url("~/Scripts/sobrescrituas.js")"></script>
    <script>
        function showModal() {
            $('.contact-us-pop-up').fadeTo('slow', 0.7, function () {
                $(this).css("display", "block");
            }).fadeTo('slow', 1);
        }

        function closeModal() {
            $('.contact-us-pop-up').fadeTo('slow', 0.7, function () {
                $(this).css("display", "none");
            }).fadeTo('slow', 1);
        }

    </script>


    @{
        var _user = Session.GetCurrentUser();

        if (_user == null)
        {
            _user = @Session.GetDataWithOutLogin();
        }

        var userPlan = Session.GetCurrenUserPlan();
        string plan = string.Empty;
        if (userPlan != null)
        {
            plan = string.Format("{0} Plan", @userPlan.PlanName);
        }

    }
    <link rel="icon" href="@_user.TenantSkin.TenantFaviconUrl">

    <!-- Start of  Zendesk Widget script -->
    <script id="ze-snippet" src="https://static.zdassets.com/ekr/snippet.js?key=b2a53c26-0233-4d53-abe2-067b1f20976b"></script>
    <!-- End of  Zendesk Widget script -->

</head>

