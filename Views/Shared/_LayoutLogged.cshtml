                    <li class="item has_btn"><a onclick="gAnalytics.throwRedirectEvent('Click Upgrade', 'Plan Upgrade','Plan Upgrade','@Url.Action("Upgrade", "Plan" )')" href="#" class="btn red" style="background-color: @_user.TenantSkin.TenantStyleTertiaryBackgroundColor; color: @_user.TenantSkin.TenantStyleTertiaryFontColor;">@TranslatorHelper.TranslateTextByIdentifier("upgrade_text")</a></li>

                }
                <li class="item"><a href="@Url.Action("Index", "Support" )" class="@{ if (ViewBag.Active == " 5") { ViewContext.Writer.Write("active"); } }" target="_self">@TranslatorHelper.TranslateTextByIdentifier("supporte_text")</a></li>
